{
  "exclude": [".sst"],
  "compilerOptions": {
    "plugins": [{ "name": "ts-sst-plugin" }],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "target": "es2022",
    "allowJs": true,
    "resolveJsonModule": true,
    "moduleDetection": "force",
    "isolatedModules": true,
    "verbatimModuleSyntax": true,
    "module": "preserve",
    "noEmit": true,
    "types": []
  }
}
